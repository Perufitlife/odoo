<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_courier_shipment" model="ir.sequence">
            <field name="name">Courier Shipment</field>
            <field name="code">courier.shipment</field>
            <field name="prefix">SHIP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_courier_shipment_tracking" model="ir.sequence">
            <field name="name">Courier Shipment Tracking</field>
            <field name="code">courier.shipment.tracking</field>
            <field name="prefix">TRACK/%(year)s/</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
